{
  "name": "mibi-portal-server",
  "version": "1.3.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./lib/main.js",
    "serve": "nodemon -e ts --exec \"npm run build && npm run test && node ./lib/main.js\"",
    "debug": "nodemon -e ts --exec \"npm run build && node --inspect-brk ./lib/main.js\"",
    "cp:view": "copyfiles -u 1 ./src/**/views/**/* ./lib",
    "cp:json": "copyfiles -u 1 ./src/**/*.json ./lib",
    "cp:scripts": "copyfiles -u 1 ./src/**/*.sh ./lib",
    "cp:all": "npm run cp:view && npm run cp:scripts && npm run cp:json",
    "tsc": "tsc",
    "build": "npm run clean &&  npm run tsc && npm run cp:all",
    "test": "jest --config=jest.json",
    "test:integration": "jest --config=jest-integration.json",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "clean": "rimraf lib/*",
    "lint": "tslint --project tsconfig.json --fix -c tslint.json 'src/**/*.ts'",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "npm run pretty",
        "npm run lint",
        "git add"
      ]
    },
    "ignore": [
      "src/**/__mocks__/*.ts",
      "src/**/__tests__/*.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "argon2": "0.19.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.3",
    "config": "^3.2.5",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fast-csv": "^2.4.1",
    "filereader": "^0.10.3",
    "fs-extra": "^8.1.0",
    "fs-readfile-promise": "^3.0.1",
    "fuse.js": "3.2.1",
    "handlebars": "^4.7.3",
    "helmet": "^3.21.2",
    "html5-history-api": "^4.2.10",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "linebyline": "^1.3.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "mongodb": "^3.5.3",
    "mongoose": "^5.4.10",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^4.4.1",
    "processenv": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.0.2",
    "validate.js": "^0.13.1",
    "winston": "^3.0.0",
    "xlsx": "^0.15.5",
    "xlsx-populate": "^1.20.1"
  },
  "devDependencies": {
    "@types/app-root-dir": "^0.1.0",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/fs-readfile-promise": "^3.0.0",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.1",
    "@types/mongoose-unique-validator": "^1.0.3",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.7.1",
    "@types/nodemailer": "^4.3.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/xlsx": "0.0.36",
    "axios": "^0.19.2",
    "copyfiles": "^2.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^8.0.4",
    "mock-express-request": "^0.2.2",
