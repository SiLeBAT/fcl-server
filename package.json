{
  "name": "fcl-portal-server",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "start": "node ./lib/main.js",
    "serve": "npx nodemon -e ts --exec \"npm run build && npm run test && node ./lib/main.js\"",
    "debug": "npx nodemon -e ts --exec \"npm run build && node --inspect-brk ./lib/main.js\"",
    "cp:view": "npx copyfiles -u 1 ./src/**/views/**/* ./lib",
    "cp:json": "npx copyfiles -u 1 ./src/**/*.json ./lib",
    "cp:scripts": "npx copyfiles -u 1 ./src/**/*.sh ./lib",
    "cp:all": "npm run cp:view && npm run cp:scripts && npm run cp:json",
    "tsc": "tsc",
    "build": "npm run clean &&  npm run tsc && npm run cp:all",
    "test": "jest --config=jest.json",
    "test:integration": "jest --config=jest-integration.json",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "clean": "npx rimraf lib/*",
    "lint": "tslint --project tsconfig.json --fix -c tslint.json -e \"src/**/__mocks__/*.ts\" -e \"src/**/__tests__/*.ts\" -e \"test/**/*.ts\" \"src/**/*.ts\"",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "private": true,
  "dependencies": {
    "@SiLeBAT/fg43-ne-server": "^1.1.0",
    "argon2": "0.28.5",
    "bluebird": "~3.7.2",
    "compression": "~1.7.4",
    "config": "~3.3.7",
    "cors": "~2.8.5",
    "express": "~4.17.3",
    "express-jwt": "^6.1.2",
    "fs-readfile-promise": "~3.0.1",
    "handlebars": "~4.7.7",
    "helmet": "~4.6.0",
    "inversify": "~5.1.1",
    "inversify-express-utils": "~6.3.2",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.21",
    "moment": "~2.29.2",
    "mongoose": "6.4.6",
    "mongoose-unique-validator": "~3.0.0",
    "morgan": "~1.10.0",
    "nodemailer": "6.7.3",
    "pm2": "~5.2.0",
    "reflect-metadata": "0.1.13",
    "request": "~2.88.2",
    "swagger-ui-express": "~4.3.0",
    "winston": "~3.3.4"
  },
  "devDependencies": {
    "@types/bluebird": "~3.5.36",
    "@types/compression": "~1.7.2",
    "@types/config": "~0.0.41",
    "@types/cors": "~2.8.12",
    "@types/express": "~4.17.13",
    "@types/express-jwt": "~6.0.4",
    "@types/fs-readfile-promise": "^3.0.1",
    "@types/helmet": "~4.0.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "~8.5.8",
    "@types/lodash": "^4.14.181",
    "@types/morgan": "~1.9.3",
    "@types/node": "~12.12.70",
    "@types/nodemailer": "~6.4.4",
    "@types/reflect-metadata": "0.1.0",
    "@types/swagger-ui-express": "~4.1.3",
    "copyfiles": "~2.4.1",
    "husky": "~7.0.4",
    "jest": "~27.4.7",
    "lint-staged": "~11.2.6",
    "mock-express-request": "0.2.2",
    "mock-express-response": "0.2.2",
    "nodemon": "~2.0.15",
    "prettier": "~2.6.2",
    "rimraf": "~3.0.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "~1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "~4.6.3"
  },
  "//": {
    "@comment dependencies": {
      "@types/jest": "'~' is used because of minor version sync with 'jest'",
      "jest": "'~' is used because of minor version sync with '@types/jest'"
    }
  },
  "fclConfig": {
